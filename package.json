    "@react-navigation/stack": "^6.2.2",
    "@reduxjs/toolkit": "^1.8.3",
    "i18next": "^21.8.14",
    "prop-types": "^15.8.1",
    "react": "18.0.0",
    "react-i18next": "^11.18.3",
    "react-native": "0.69.3",
    "react-native-flipper": "^0.156.0",
